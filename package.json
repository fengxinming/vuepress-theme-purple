{
  "name": "vuepress-theme-purple",
  "version": "1.0.0",
  "private": true,
  "description": "A purple theme for Vuepress",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "tyarn install --no-lockfile",
    "vuepress-docs": "yarn run --cwd=./packages/vuepress-docs",
    "release": "npm publish ./packages/vuepress-theme-purple",
    "gh-pages": "git subtree push --prefix=docs origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fengxinming/vuepress-theme-purple.git"
  },
  "author": "Jesse Feng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fengxinming/vuepress-theme-purple/issues"
  },
  "homepage": "https://github.com/fengxinming/vuepress-theme-purple#readme"
}